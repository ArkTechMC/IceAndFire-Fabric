{
  "schemaVersion": 1,
  "id": "iceandfire",
  "version": "${version}",
  "name": "Ice And Fire",
  "description": "The unofficial Fabric version of Ice And Fire.\nDragons and other mythological creatures in minecraft.",
  "authors": [
    "Alexthe666 (Origin Author)",
    "IAFEnvoy"
  ],
  "contact": {
    "repo": "https://github.com/IAFEnvoy/IceAndFire-Fabric"
  },
  "license": "LGPL-3.0",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.iafenvoy.iceandfire.datagen.ModDataGenerator"
    ],
    "main": [
      "com.iafenvoy.iceandfire.IceAndFire"
    ],
    "client": [
      "com.iafenvoy.iceandfire.IceAndFireClient"
    ],
    "emi": [
      "com.iafenvoy.iceandfire.compat.emi.IceAndFireEmiPlugin"
    ],
    "jade": [
      "com.iafenvoy.iceandfire.compat.jade.IceAndFireJadePlugin"
    ],
    "cardinal-components-entity": [
      "com.iafenvoy.iceandfire.data.ModComponentEntry"
    ]
  },
  "custom": {
    "cardinal-components": [
      "iceandfire:entity_data"
    ]
  },
  "mixins": [
    "citadel.mixins.json",
    "iceandfire.mixins.json"
  ],
  "accessWidener": "iceandfire.accesswidener",
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "1.20.1"
  }
}
